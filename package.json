{
  "name": "eslint-plugin-diff",
  "version": "1.0.7",
  "description": "Run ESLint on your changes only",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "diff"
  ],
  "homepage": "https://github.com/paleite/eslint-plugin-diff#readme",
  "bugs": {
    "url": "https://github.com/paleite/eslint-plugin-diff/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paleite/eslint-plugin-diff.git"
  },
  "license": "MIT",
  "author": "Patrick Eriksson <paleite@users.noreply.github.com> (https://github.com/paleite)",
  "main": "dist/index.js",
  "files": [
    "/dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "tsc --build tsconfig.build.json --clean",
    "format": "prettier --write .",
    "lint": "eslint --cache --ext .js,.ts --fix --format codeframe .",
    "release": "np",
    "test": "jest --coverage",
    "typecheck": "tsc --project tsconfig.json",
    "prepublish": "yarn run clean && yarn run build",
    "postpublish": "pinst --enable",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable"
  },
  "devDependencies": {
    "@tsconfig/node10": "^1.0.7",
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "np": "^7.4.0",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "eslint": ">=6.7.0"
  },
  "engines": {
    "node": ">=10.23.0"
  }
}
