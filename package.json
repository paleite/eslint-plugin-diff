{
  "name": "eslint-plugin-diff",
  "version": "2.1.5",
  "description": "Run ESLint on your changes only",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "diff"
  ],
  "homepage": "https://github.com/paleite/eslint-plugin-diff#readme",
  "bugs": {
    "url": "https://github.com/paleite/eslint-plugin-diff/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paleite/eslint-plugin-diff.git"
  },
  "license": "MIT",
  "author": "Patrick Aleite <paleite@users.noreply.github.com> (https://github.com/paleite)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    ".github/ISSUE_TEMPLATE/feature_request.md",
    "/dist/",
    "CODE_OF_CONDUCT.md",
    "SECURITY.md"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "bench:preprocess-membership": "node scripts/benchmark-preprocessor-membership.ts",
    "clean": "tsc --build tsconfig.build.json --clean",
    "format": "prettier --write .",
    "lint": "eslint --cache --ext .js,.ts --fix .",
    "prepack": "pnpm run clean && pnpm run build",
    "lint:package": "pnpm run prepack && pnpm dlx publint --strict --pack pnpm",
    "lint:types": "pnpm run prepack && pnpm dlx @arethetypeswrong/cli --pack .",
    "release": "pnpm run lint:types && np --no-publish",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prepare": "husky && (git rev-parse --is-inside-work-tree >/dev/null 2>&1 && git config blame.ignoreRevsFile .git-blame-ignore-revs || true)"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@paleite/eslint-config": "^2.0.1",
    "@paleite/eslint-config-base": "^2.0.1",
    "@paleite/eslint-config-typescript": "^2.0.1",
    "@paleite/jest-config": "^2.0.0",
    "@paleite/prettier-config": "^2.0.0",
    "@paleite/tsconfig-node-lts": "^2.0.0",
    "@paleite/tsconfig-strictest": "^2.0.0",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "np": "^7.7.0",
    "prettier": "^3.8.1",
    "pretty-quick": "^4.2.2",
    "ts-jest": "^29.4.6",
    "tsd": "^0.25.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "eslint": ">=6.7.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc"
}
